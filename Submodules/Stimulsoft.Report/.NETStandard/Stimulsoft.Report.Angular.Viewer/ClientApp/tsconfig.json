{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "stimulsoft-viewer-angular": [
        "dist/stimulsoft-viewer-angular/stimulsoft-viewer-angular",
        "dist/stimulsoft-viewer-angular"
      ],
      "stimulsoft-designer-angular": [
        "dist/stimulsoft-designer-angular/stimulsoft-designer-angular",
        "dist/stimulsoft-designer-angular"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}